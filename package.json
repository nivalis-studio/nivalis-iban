{
  "name": "@nivalis/iban",
  "version": "0.1.0",
  "description": "A simple library to validate IBAN numbers",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "type": "module",
  "homepage": "https://github.com/nivalis-studio/iban#readme",
  "repository": {
    "url": "git+https://github.com/nivalis-studio/iban.git"
  },
  "bugs": {
    "url": "https://github.com/nivalis-studio/iban/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "bun@1.2.19",
  "scripts": {
    "test": "bun test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts": "tsc",
    "build": "zshy",
    "prepack": "zshy",
    "prepare": "bunx lefthook install"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nivalis/eslint-config": "5.5.1",
    "@nivalis/prettier-config": "3.6.2",
    "@types/bun": "latest",
    "eslint": "9.32.0",
    "lefthook": "1.12.2",
    "lint-staged": "16.1.4",
    "prettier": "3.6.2",
    "zshy": "0.3.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "lint-staged": {
    "*": "eslint --quiet --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "zshy": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
