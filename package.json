{
	"name": "@nivalis/iban",
	"version": "0.3.2",
	"description": "A simple library to validate IBAN numbers",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"type": "module",
	"homepage": "https://github.com/nivalis-studio/iban#readme",
	"repository": {
		"url": "https://github.com/nivalis-studio/nivalis-iban"
	},
	"bugs": {
		"url": "https://github.com/nivalis-studio/iban/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"sideEffects": false,
	"packageManager": "pnpm@10.23.0",
	"scripts": {
		"test": "bun test",
		"build": "zshy",
		"prepack": "zshy",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"prepare": "lefthook install",
		"ts": "tsc --noEmit"
	},
	"files": ["LICENSE", "README.md", "dist"],
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
		"@nivalis/biome-config": "2.3.7",
		"@total-typescript/ts-reset": "0.6.1",
		"lefthook": "2.0.4",
		"typescript": "5.9.3",
		"zshy": "0.5.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"zshy": "./src/index.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	}
}
